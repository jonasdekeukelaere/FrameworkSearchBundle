    "symfony/event-dispatcher": "2.6.*",
    "doctrine/orm": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.*"
  },
  "autoload": {
    "psr-0": { "SumoCoders\\FrameworkSearchBundle": "" }
  },
  "target-dir": "SumoCoders/FrameworkSearchBundle",
  "config": {
    "bin-dir": "bin"
  }
